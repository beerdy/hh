<p id="notice"><%= notice %></p>

<h1>Messages</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Slave</th>
      <th>Image</th>
      <th>Link</th>
      <th>tag</th>
      <th>Url</th>
      <th>Link</th>
      <th>Sort</th>
      <th>Count</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @messages.each do |message| %>
      <tr>
        <td><%= message.title %></td>
        <td><%= message.description %></td>
        <td><%= message.slave %></td>
        <td><%= message.image %></td>
        <td><%= message.link %></td>
        <td><%= message.tag %></td>
        <td><%= message.url %></td>
        <td><%= message.link %></td>
        <td><%= message.sort %></td>
        <td><%= message.count %></td>
        <td><%= link_to 'Show', message %></td>
        <td><%= link_to 'Edit', edit_message_path(message) %></td>
        <td><%= link_to 'Destroy', message, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Message', new_message_path %>
