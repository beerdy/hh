<p id="notice"><%= notice %></p>

<h1>Histories</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Slave</th>
      <th>Image</th>
      <th>Link</th>
      <th>tag</th>
      <th>Url</th>
      <th>Link</th>
      <th>Sort</th>
      <th>Count</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @histories.each do |history| %>
      <tr>
        <td><%= history.title %></td>
        <td><%= history.description %></td>
        <td><%= history.slave %></td>
        <td><%= history.image %></td>
        <td><%= history.link %></td>
        <td><%= history.tag %></td>
        <td><%= history.url %></td>
        <td><%= history.link %></td>
        <td><%= history.sort %></td>
        <td><%= history.count %></td>
        <td><%= link_to 'Show', history %></td>
        <td><%= link_to 'Edit', edit_history_path(history) %></td>
        <td><%= link_to 'Destroy', history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New History', new_history_path %>
